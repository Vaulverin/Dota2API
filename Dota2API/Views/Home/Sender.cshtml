@model Dota2API.Models.NewsModel

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript">
        $('#send').click(function () {
            var destination = $('#url').val();
            //alert($("form").serializeArray());
            $.post('/api/querysender/', $("form").serialize(), null, "json").then(
                function (result) { console.log(JSON.parse(result)); $('#result').html("SUCCESS"); },
                function (error) { $('#result').html("FAIL : " + error); });
            return false;
        });
    </script>
}

@{
    ViewBag.Title = "Query Sender";
}

<h2>@ViewBag.Title</h2>
<div class="content">
    <form>
        <input type="text" id="Url" name="Url" />
        <button id="send">Send</button>
    </form>
    <div id="result"></div>
</div>